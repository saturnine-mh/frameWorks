</section>

<div  id="profileMainContent"  class="container">
  <div class="row mt-5">
      <div class="col-6">
          <div class="mt-5">
            <section id="postBox">
            <h1 id="profileHeroAdd" class="font-bold text-2xl text-white">Add an image to compress!</h1>
            <form action="/post/createPost" enctype="multipart/form-data" method="POST">
              <div>
                <label for="title" class="block mb-2 text-sm font-medium text-white-900 text-white">Quality (1-100)</label>
                <input type="number" id="title" name="title" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="1-100%" >
            </div>
            <div>
              <label for="album" class="block mb-2 text-sm font-medium text-white-900 text-white">Album</label>
              <input type="text" id="album" name="album" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="Insert Album Name" >
          </div>
          <div>
            <label for="name" class="block mb-2 text-sm font-medium text-white-900 text-white">Media Name</label>
            <input type="text" id="name" name="name" class="bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" placeholder="Give Your Media A Name" >
        </div>
              
              <div >
              

<div id="postUploadBox" class="flex items-center justify-center w-full">
  <label for="imgUpload" class="flex flex-col items-center justify-center w-full h-64 border-2 border-gray-300 border-dashed rounded-lg cursor-pointer bg-gray-50 dark:hover:bg-bray-800 dark:bg-gray-700 hover:bg-gray-100 dark:border-gray-600 dark:hover:border-gray-500 dark:hover:bg-gray-600">
      <div class="flex flex-col items-center justify-center pt-5 pb-6">
          <svg aria-hidden="true" class="w-10 h-10 mb-3 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12"></path></svg>
          <p class="mb-2 text-sm text-gray-500 dark:text-gray-400"><span class="font-semibold">Click to upload</span> or drag and drop</p>
          <p class="text-xs text-gray-500 dark:text-gray-400">SVG, PNG, JPG or GIF (MAX. 5MB)</p>
      </div>
      <input aria-describedby="file_input_help" id="imgUpload" type="file" name="file" class="hidden" />
  </label>
</div> 

              </div>
              <button id="postSubmit" type="submit" class="text-white bg-blue-800 hover:bg-blue-900 focus:outline-none focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:bg-blue-800 dark:hover:bg-blue-700 dark:focus:ring-blue-700 dark:border-blue-700" value="Upload">Submit</button>
              


            </form>
          </section>
          </div>
          
      </div>
      <div class="col-6">
        <ul class="row list-unstyled">
          <div class="row justify-content-center mt-5">
            <a id="profileFeedText" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800" href="/feed">View All Albums</a>
            
          </div>  
          <section id="profileAllPhotos">
          <h1 id="" class="font-bold text-2xl">All Photos</h1>
        <% for(var i=0; i<posts.length; i++) {%>
<ul class="max-w-md divide-y divide-gray-200 dark:divide-gray-700">
  <li class="pb-3 sm:pb-4">
     <div class="flex items-center space-x-4">
        <div class="flex-shrink-0">
          <a href="/post/<%= posts[i]._id%>">
           <img class="w-8 h-8 rounded-full" src="<%= posts[i].image %>" alt="Neil image">
          </a>
        </div>
        <div class="flex-1 min-w-0">
           <a href="/post/<%= posts[i]._id%>" class="text-sm font-medium text-gray-900 truncate dark:text-white">
              NAME: <%= posts[i].name %>
           </a>
           <a href="/albums/<%= posts[i].album %>" class="text-sm font-medium text-gray-900 truncate dark:text-white">
            ALBUM: <%= posts[i].album %>
         </a>
           <p class="text-sm text-gray-500 truncate dark:text-gray-400">
              <%= posts[i].createdAt %>
           </p>
        </div>
        <div class="inline-flex items-center text-base font-semibold text-blue-900 text-blue">
          QUALITY: <%= posts[i].title %>
        </div>
     </div>
  </li>
 

<% } %>
</section>
      </div>  
    </div>
  </div>
</div>
<%- include('partials/footer') -%>